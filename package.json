{
  "name": "SBAsset6",
  "version": "2.0.1",
  "description": "SBAsset6 parsing library",
  "main": "build/SBAsset6.js",
  "types": "build/SBAsset6.d.ts",
  "scripts": {
    "style": "standard *.ts",
    "unit": "mocha --compilers ts:ts-node/register ./test/*.spec.ts",
    "regression": "mocha --compilers ts:ts-node/register ./test/*.regression.ts",
    "test": "npm run unit && npm run regression",
    "build": "tsc",
    "qc": "npm run build && npm run style && npm run unit && npm run regression"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/damianb/sbasset6.git"
  },
  "keywords": [
    "starbound",
    "SBAsset6"
  ],
  "author": "Damian Bushong <katana@odios.us>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/damianb/sbasset6/issues"
  },
  "homepage": "https://github.com/damianb/sbasset6#readme",
  "dependencies": {
    "ConsumableBuffer": "github:damianb/ConsumableBuffer",
    "ConsumableFile": "github:damianb/ConsumableFile",
    "ExpandingBuffer": "github:damianb/ExpandingBuffer",
    "ExpandingFile": "github:damianb/ExpandingFile",
    "SBON": "github:damianb/SBON",
    "StreamPipeline": "github:damianb/streampipeline",
    "fs-extra": "^4.0.2",
    "int64-buffer": "^0.1.9"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^3.5.3",
    "standard": "^10.0.3"
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "plugins": [
      "typescript"
    ]
  }
}
